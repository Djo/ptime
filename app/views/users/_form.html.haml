-form_for @user, :url => user_path do |f|
  != f.error_messages
  %ul
    %li
      != f.label :name
      != f.text_field :name
    %li
      != f.label :email
      != f.text_field :email
    %li
      != f.label :password
      != f.password_field :password
    %li
      != f.label :password_confirmation
      != f.password_field :password_confirmation
    %li
      %label &nbsp;
      != f.submit 'save'
%br
